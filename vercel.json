{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    { "source": "/api/listings", "destination": "/api/index.js" },
    { "source": "/api/listings/:id", "destination": "/api/index.js" },
    { "source": "/api/contact", "destination": "/api/index.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
